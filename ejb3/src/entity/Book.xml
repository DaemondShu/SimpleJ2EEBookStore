<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="entity.Book">
        <table name="book" schema="bookstore" catalog=""/>
        <attributes>
            <id name="bookId">
                <column name="bookId" precision="10"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="45"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" length="45"/>
            </basic>
            <basic name="price">
                <column name="price" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
