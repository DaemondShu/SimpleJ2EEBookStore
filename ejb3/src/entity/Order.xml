<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="entity.Order">
        <table name="order" schema="bookstore" catalog=""/>
        <attributes>
            <id name="orderId">
                <column name="orderId" precision="10"/>
            </id>
            <basic name="userId">
                <column name="userId" nullable="false" precision="10"/>
            </basic>
            <basic name="bookId">
                <column name="bookId" nullable="false" precision="10"/>
            </basic>
            <basic name="address">
                <column name="address" nullable="false" length="45"/>
            </basic>
            <basic name="time">
                <column name="time" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
